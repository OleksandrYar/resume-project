<section>
    <div class="card">
        <div class="card-body">
            {{#with heading.main}}
            <h1>{{title}}</h1>
            <p class="fw-normal">{{text}}.</p>
            {{#with button_text}}
            <button type="button" class="btn btn-primary">{{solid}}</button>
            <br>
            <br>
            <br>
            <button type="button" class="btn btn-outline-primary">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
</section>
<br>
<section>
    <div class="card">
        <div class="card-body">
            {{#with heading.second}}
            <h2>{{title}}</h2>
            <p class="fw-normal">{{text}}.</p>
            {{#with button_text}}
            <button type="button" class="btn btn-secondary">{{solid}}</button>
            <br>
            <br>
            <button type="button" class="btn btn-outline-secondary">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
</section>
<br>
<section>
    <div class="card">
        <div class="card-body">
            {{#with heading.subblock}}
            <h3>{{title}}</h3>
            <p class="fw-normal">{{text}}.</p>
            {{#with button_text}}
            <button type="button" class="btn btn-dark">{{solid}}</button>
            <br>
            <br>
            <button type="button" class="btn btn-outline-dark">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
</section>
<br>
<hr>
<section>
    <div class="card">
        <div class="card-body">
            {{#with nested_display}}
            <div class="display-1">{{title}}</div>
            <div class="card">
                <div class="card-body">
                    {{#with children_display}}
                    <div class="display-4">{{title}}</div>
                    <p class="fw-light">{{text}}</p>
                    <button type="button" class="btn btn-outline-warning">{{button.text}}</button>
                    {{/with}}
                </div>
            </div>
            <br>
            <h6>{{header_text}}</h6>
            <br>
            <button type="button" class="btn btn-info">{{button.text}}</button>
            {{/with}}
        </div>
    </div>
</section>
<section>
    <div class="card">
        <div class="card-body">
            {{#with paragraph}}
            <p class="fs-3">{{title}}</p>
            <p class="fs-5">{{text}}</p>
            {{#with button}}
                    <a href="{{url}}" class="btn btn-link">{{text}}</a>
                    {{/with}}
            <div class="card">
                <div class="card-body">
                    {{#each list_name}}
                        <ul>
                            <li>
                                <a href="{{url}}" class="btn btn-link">{{text}}</a>
                            </li>
                        </ul>
                    {{/each}}
                </div>
            </div>
            {{/with}}
        </div>
    </div>
</section>